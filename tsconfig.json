{
    "compilerOptions": {
        "target": "ESNext",
        "strict": true,
        "jsx": "react-jsx",
        "moduleResolution": "Node" /* node_modules */,
        "module": "ESNext" /* import.meta */,
        "types": ["vite/client"] /* import.meta.env */,
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "noEmit": true,
        "outDir": "./lib",
        "declaration": true
    },
    "include": ["./src"]
}
